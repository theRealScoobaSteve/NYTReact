{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": 0
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "parser": "babel-eslint"
}
